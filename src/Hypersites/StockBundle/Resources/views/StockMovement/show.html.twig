{% extends '::base.html.twig' %}

{% block body -%}
    <h1>StockMovement</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Kindmovement</th>
                <td>{{ entity.kindMovement }}</td>
            </tr>
            <tr>
                <th>Movedby</th>
                <td>{{ entity.movedBy }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ entity.description }}</td>
            </tr>
            <tr>
                <th>Movementdate</th>
                <td>{{ entity.movementDate|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('stock_movement') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('stock_movement_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
